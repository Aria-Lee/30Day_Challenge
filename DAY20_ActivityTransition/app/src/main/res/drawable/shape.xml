<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">
    <!-- Corner的属性是设置圆角的半径的-->
    <corners
            android:radius="10dp"/>
    <!-- Button里面的文字与Button边界的间隔-->
    <padding
            android:bottom="10dp"
            android:left="10dp"
            android:right="10dp"
            android:top="10dp"/>
    <solid android:color="#FFFFBB"/>
</shape>